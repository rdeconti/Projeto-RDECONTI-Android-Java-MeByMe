<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.prosper.day">

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BATTERY_STATE" />
    <uses-permission android:name="ACCESS_WIFI_STATE" />
    <uses-permission android:name="CAMERA" />

    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/ThemeDefault"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>

        <activity
            android:name=".module00splash.ModuleSplashActivity"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module01main.ModuleControlMainActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module66authorization.AuthorizationApplicationEmailActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module66authorization.AuthorizationPasswordChangeActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module66authorization.AuthorizationApplicationLogoutActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module66authorization.AuthorizationPasswordResetActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module66authorization.AuthorizationApplicationSigninActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module66authorization.AuthorizationApplicationSignoutActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module66authorization.AuthorizationApplicationLoginActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".module88setting.NavigationSettingsActivity"
            tools:ignore="InnerclassSeparator" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>